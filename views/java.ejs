<%- include('navbar'); -%>
<%- include('header'); -%>
 

<style>
    #javacontent{
        width: 90%;
        height: 80vh;
        padding: 10px;
        overflow-x: scroll;
        overflow-y: scroll;
        border: 1px solid rgb(63, 63, 63);
        border-radius: 5px;
        margin-left: 5%;
    }

    .java-container{
        padding: 10px;
        
    }

    .card{
        width: 90%;
        height: 550px;
        background: rgb(146, 146, 146);
        border-radius: 7px;
        border: none;
        overflow-x: scroll;
        overflow-y: scroll;
        margin-left: 5%;
        margin-bottom: 20px;
    }

    .card::-webkit-scrollbar{
        display: none;
    }

    @media screen and (max-width:750px){
        .card{
            width: 98%;
            margin-left: 1%;
            height: 90vh;
        }
    }
</style>

<section id="javacontent">
    <div class="java-container">
        <div class="card">
            <h1>Quick revision</h1>
            <embed src="../doc/java.pdf"  width="100%" height="100%" type="application/pdf">
        </div>
    </div>
</section>


